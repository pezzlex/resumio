{
  "name": "resumio",
  "description": "Your resume in the cloud",
  "version": "3.0.6",
  "private": true,
  "author": "Pezanne Khambatta",
  "scripts": {
    "build": "react-scripts build",
    "start": "node server.js",
    "heroku-postbuild": "yarn && yarn run build",
    "dev": "concurrently \"nodemon server \" \"react-scripts start\""
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,md,css}": [
      "prettier --trailing-comma es5 --single-quote --write"
    ]
  },
  "devDependencies": {
    "concurrently": "^5.2.0",
    "cross-env": "^7.0.2",
    "husky": "^4.2.5",
    "lerna": "^3.22.1",
    "lint-staged": "^10.2.10",
    "prettier": "^2.0.5",
    "redux-devtools-extension": "^2.13.8",
    "rimraf": "^3.0.2",
    "serve": "^11.3.2"
  },
  "dependencies": {
    "@ant-design/icons": "4.0.0",
    "@react-pdf/renderer": "^1.6.10",
    "antd": "^4.5.2",
    "axios": "^0.19.2",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-jwt": "^6.0.0",
    "joi": "^17.1.1",
    "jwt-simple": "^0.5.6",
    "lodash": "^4.17.19",
    "mongoose": "^5.9.28",
    "node-sass": "^4.14.1",
    "nodemailer": "^6.4.10",
    "nodemon": "^2.0.4",
    "pspdfkit": "^2020.4.1",
    "react-custom-scrollbars": "^4.2.1",
    "react-dom": "^16.13.1",
    "react-pdf": "^4.2.0",
    "react-redux": "^7.2.1",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^3.4.1",
    "redux": "^4.0.5",
    "redux-localstorage-simple": "^2.3.1",
    "redux-thunk": "^2.3.0",
    "remote-redux-devtools": "^0.5.16",
    "styled-components": "^5.1.1",
    "styled-theme": "^0.3.3"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
